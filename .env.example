# Rotating Tor HTTP Proxy Configuration

# Number of Tor instances to run (default: 20)
TOR_PROXY_TOR_INSTANCES=20

# Starting port for SOCKS/Control allocation (default: 10000)
TOR_PROXY_TOR_BASE_PORT=10000

# Upper bound for allocated SOCKS/Control ports (default: 10799)
TOR_PROXY_TOR_MAX_PORT=10799

# Max parallel startups per batch (default: 20)
TOR_PROXY_TOR_START_BATCH=20

# Timeout for Tor instance startup (default: 90.0 seconds)
TOR_PROXY_TOR_START_TIMEOUT_SECONDS=90.0

# Number of retries for failed Tor startups (default: 2)
TOR_PROXY_TOR_START_RETRIES=2

# Delay between Tor startup retries (default: 5.0 seconds)
TOR_PROXY_TOR_START_RETRY_DELAY_SECONDS=5.0

# Directory for Tor data (default: ./data)
TOR_PROXY_TOR_DATA_DIR=./data

# Frontend port for the proxy (default: 9999)
TOR_PROXY_FRONTEND_PORT=9999

# URL for health checks (default: https://httpbin.org/ip)
TOR_PROXY_HEALTH_CHECK_URL=https://httpbin.org/ip

# Steam health check URL (default: https://steamcommunity.com/market/listings/730/AK-47)
TOR_PROXY_STEAM_HEALTH_URL=https://steamcommunity.com/market/listings/730/AK-47

# Timeout for health checks (default: 10.0 seconds)
TOR_PROXY_HEALTH_TIMEOUT_SECONDS=10.0

# Number of retries for health checks (default: 3)
TOR_PROXY_HEALTH_RETRIES=3

# Interval between health checks (default: 60.0 seconds)
TOR_PROXY_HEALTH_INTERVAL_SECONDS=60.0

# Log level (default: INFO)
TOR_PROXY_LOG_LEVEL=INFO

# Enable verbose logging (default: false)
TOR_PROXY_LOG_VERBOSE=false

# Number of exit nodes per instance (default: 0 - disabled)
TOR_PROXY_EXIT_NODES_PER_INSTANCE=0

# Global cap on exit nodes fetched (default: 0 - disabled)
TOR_PROXY_EXIT_NODES_MAX=0